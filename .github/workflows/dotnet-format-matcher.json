{
  "problemMatcher": [
    {
      "owner": "csc",
      "pattern": [
        {
          "regexp": "^\\s*(.*)\\((\\d+),(\\d+)\\):\\s+(.*)\\s+(.*):\\s+(.*)\\s+\\[(.*)\\]$",
          "file": 1,
          "line": 2,
          "column": 3,
          "code": 5,
          "message": 6,
          "fromPath": 7
        }
      ]
    }
  ]
}